{
  "manifest_version": 3,
  "name": "Tab Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_title": "click to open panel"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": [
    "sidePanel",
    "tabs", 
    "activeTab"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
